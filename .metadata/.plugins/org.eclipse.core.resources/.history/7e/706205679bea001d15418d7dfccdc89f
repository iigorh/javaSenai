package principal;

public class AulaVetorMatriz {

	public static void main(String[] args) {
		
		int [] numero = new int [5];
		numero [0] = 100;
		numero [1] = 20;
		numero [2] = 15;
		numero [3] = 55;
		numero [4] = 66;
		//numero [299] = 15;
		System.out.println(numero[0]);
		
		int[] numeros = {1, 2, 3, 10, 50, 60, 80};
		
		System.out.println(numeros[3]);
	}

}
