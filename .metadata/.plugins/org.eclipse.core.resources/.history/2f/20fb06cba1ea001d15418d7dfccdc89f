		// primeiro colchetes quantidade de linhas e segundo a quantidade de colunas
		int[][] mult = new int [3][3];
		int contador = 0;
		for(int i = 0; i < mult.length; i++) {
			for(int j = 0; j < mult.length; j++) {
				mult[i][j] = contador;
				System.out.println(mult[i][j]+ "\t");
				contador++;
			}
		}
		
		
	}

}
