		Random rd = new Random();
		
		int[] numeros = new int[6]; 
		System.out.println("Os números sorteados são:");
		for (int i = 0; i < 6; i++) {
			numeros[i] = rand.nextInt(60) + 1; // gera um número aleatório entre 1 e 60         System.out.println(numeros[i]);      }   }}
		}
