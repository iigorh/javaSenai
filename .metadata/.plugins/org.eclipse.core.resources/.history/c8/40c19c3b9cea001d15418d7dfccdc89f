		int[] numeros = {1, 2, 3, 10, 50, 60, 80};
		
		System.out.println(numeros[3]);
		System.out.println(numeros.length);
		
		for(int contador = 0; contador <numeros.length; contador++) {
			System.out.println(numeros[0]);
		}
	}

}
