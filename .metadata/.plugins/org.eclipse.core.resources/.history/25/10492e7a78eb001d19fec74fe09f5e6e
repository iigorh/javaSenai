package principal;

import java.util.Random;

public class Program {

	public static void main(String[] args) {
		Random rd = new Random(); 
		int[] sorteio = new int[7];
		
		
		for(int i = 0; i < 7; i++) {
		 sorteio[i] = rd.nextInt(61);		
		}
		for(int i = 0;i<6;i++) {
			System.out.println("Números sorteados: " + sorteio[i]+ "\t");
		
		}
		
	}
}	